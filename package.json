{
  "name": "rainbow-overrides",
  "version": "1.4.0",
  "type": "module",
  "module": "dist/rainbow-overrides.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src",
    "rainbow-overrides.json"
  ],
  "exports": {
    ".": "./dist/rainbow-overrides.esm.js",
    "./offline-overrides": "./rainbow-overrides.json"
  },
  "engines": {
    "node": ">=14"
  },
  "repository": "git@github.com:ctjlewis/rainbow-overrides.git",
  "author": "C. Lewis <ctj.lewis@icloud.com>",
  "license": "MIT",
  "private": false,
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "tsdx build --format esm",
    "write": "yarn build && node scripts/write.mjs",
    "pre-commit": "yarn write && git add --all && git stage --all"
  },
  "dependencies": {
    "glob": "^7.1.7",
    "rainbow-token-list-test": "^1.31.0",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "husky": "^6.0.0",
    "tsdx": "^0.14.1"
  }
}
