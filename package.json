{
  "name": "rainbow-overrides",
  "version": "1.12.0",
  "typings": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist"],
  "engines": {
    "node": ">=14"
  },
  "repository": "git@github.com:ctjlewis/rainbow-overrides.git",
  "author": "C. Lewis <ctj.lewis@icloud.com>",
  "license": "MIT",
  "private": false,
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "scripts": {
    "build": "tsdx build",
    "write": "yarn build && node scripts/write.mjs",
    "postwrite": "yarn build",
    "test": "tsdx test",
    "posttest": "node test/import.mjs && node test/require.cjs",
    "prepare": "yarn write"
  },
  "dependencies": {
    "glob": "^7.1.7",
    "typescript": "^4.3.4"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "tsdx": "ctjlewis/tsdx"
  }
}
